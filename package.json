{
  "name": "tree-sitter-iitran",
  "version": "1.0.0",
  "description": "IITRAN grammar for tree-sitter",
  "main": "bindings/node",
  "keywords": [
    "parser",
    "lexer"
  ],
  "scripts": {
    "build": "tree-sitter generate",
    "parse": "tree-sitter parse",
    "test": "tree-sitter test",
    "install": "node-gyp rebuild"
  },
  "author": "My Dinh",
  "license": "MIT",
  "dependencies": {
    "nan": "^2.16.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/b72u68/tree-sitter-iitran.git"
  },
  "tree-sitter": [
    {
      "scope": "source.iit",
      "file-types": [
        "iit"
      ],
      "injection-regex": "^(iit|iitran)$"
    }
  ],
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/b72u68/tree-sitter-iitran/issues"
  },
  "homepage": "https://github.com/b72u68/tree-sitter-iitran#readme",
  "directories": {
    "test": "tests"
  }
}
